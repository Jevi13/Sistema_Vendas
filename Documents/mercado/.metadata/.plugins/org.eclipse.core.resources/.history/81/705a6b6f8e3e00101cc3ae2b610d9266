	public Leite(String nome, double preco, TipoUnidade tipoVenda) {
		super(nome, preco, tipoVenda);
		
	}

	@Override
	public void exibirDetalhes() {
		// TODO Auto-generated method stub
		System.out.println("Produto: " + getNome() + " | Pre√ßo: R$" + getPreco() + " | Estoque: " + getEstoque() + " " + getTipoVenda());
	}

}
